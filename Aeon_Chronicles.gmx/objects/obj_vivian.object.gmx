    image_speed = 0.15;
} else if (y &lt; yprevious) {
    if (sprite_index != spr_vivian_up)
        sprite_index= spr_vivian_up;
    image_speed = 0.15;
} else {
    image_index = 0;
    image_speed = 0;
}

</string>
