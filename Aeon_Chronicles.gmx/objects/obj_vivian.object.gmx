    image_speed = 0.15;
}

// 4 directional movement

else if (x &gt; xprevious) {
    if (sprite_index != spr_vivian_right)
        sprite_index= spr_vivian_right;
    image_speed = 0.15;
} else if (x &lt; xprevious) {
    if (sprite_index != spr_vivian_left)
        sprite_index= spr_vivian_left;
    image_speed = 0.15;
} else if (y &gt; yprevious) {
