        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>add_char = "";

// end game
if keyboard_check(vk_escape) game_end();

// letters
if keyboard_check_pressed(ord("A")) add_char = "a";
if keyboard_check_pressed(ord("B")) add_char = "b";
if keyboard_check_pressed(ord("C")) add_char = "c";
if keyboard_check_pressed(ord("D")) add_char = "d";
if keyboard_check_pressed(ord("E")) add_char = "e";
if keyboard_check_pressed(ord("F")) add_char = "f";
if keyboard_check_pressed(ord("G")) add_char = "g";
if keyboard_check_pressed(ord("H")) add_char = "h";
if keyboard_check_pressed(ord("I")) add_char = "i";
if keyboard_check_pressed(ord("J")) add_char = "j";
if keyboard_check_pressed(ord("K")) add_char = "k";
if keyboard_check_pressed(ord("L")) add_char = "l";
if keyboard_check_pressed(ord("M")) add_char = "m";
if keyboard_check_pressed(ord("N")) add_char = "n";
if keyboard_check_pressed(ord("O")) add_char = "o";
if keyboard_check_pressed(ord("P")) add_char = "p";
if keyboard_check_pressed(ord("Q")) add_char = "q";
if keyboard_check_pressed(ord("R")) add_char = "r";
if keyboard_check_pressed(ord("S")) add_char = "s";
if keyboard_check_pressed(ord("T")) add_char = "t";
if keyboard_check_pressed(ord("U")) add_char = "u";
if keyboard_check_pressed(ord("V")) add_char = "v";
if keyboard_check_pressed(ord("W")) add_char = "w";
if keyboard_check_pressed(ord("X")) add_char = "x";
if keyboard_check_pressed(ord("Y")) add_char = "y";
if keyboard_check_pressed(ord("Z")) add_char = "z";

// numbers
if keyboard_check_pressed(ord("0")) add_char = "0";
if keyboard_check_pressed(ord("1")) add_char = "1";
if keyboard_check_pressed(ord("2")) add_char = "2";
if keyboard_check_pressed(ord("3")) add_char = "3";
if keyboard_check_pressed(ord("4")) add_char = "4";
if keyboard_check_pressed(ord("5")) add_char = "5";
if keyboard_check_pressed(ord("6")) add_char = "6";
if keyboard_check_pressed(ord("7")) add_char = "7";

// specials
if keyboard_check_pressed(ord(".")) || keyboard_check_pressed(190) add_char = ".";
if (keyboard_check_pressed(ord("/")) || keyboard_check_pressed(191)) &amp;&amp; (keyboard_check(vk_lshift) || keyboard_check(vk_rshift)) add_char = "?";
if keyboard_check_pressed(ord("1")) &amp;&amp; (keyboard_check(vk_lshift) || keyboard_check(vk_rshift)) add_char = "!";
if keyboard_check_pressed(vk_space) add_char = " ";
if keyboard_check_pressed(vk_tab) add_char = "~";
if keyboard_check_pressed(vk_enter) || (keyboard_check_pressed(ord("3")) &amp;&amp; (keyboard_check(vk_lshift) || keyboard_check(vk_rshift))) add_char = "#";

// upper case
if keyboard_check(vk_lshift) || keyboard_check(vk_rshift) add_char = string_upper(add_char);

// delete
if (keyboard_check_pressed(vk_backspace))
{
    veneer_message = string_copy(veneer_message, 1, string_length(veneer_message)-1);
    can_delete = false;
    alarm_set(0, 10);
}
if (keyboard_check(vk_backspace) &amp;&amp; can_delete) {
    veneer_message = string_copy(veneer_message, 1, string_length(veneer_message)-1);
    can_delete = false;
    alarm_set(0, 3);
}

// change text size
if keyboard_check_pressed(vk_subtract)
{
    if (veneer_size &gt; 3) veneer_size /= 3;
}
if keyboard_check_pressed(vk_add) veneer_size *= 3;

veneer_message += add_char;
</string>
          </argument>
        </arguments>
      </action>
